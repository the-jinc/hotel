# Example production environment overrides for docker-compose
# Copy to .env.production (never commit actual secrets)

# JWT secret used by the backend API
JWT_SECRET=top-secret-change-me

# Public frontend base URL used by backend callbacks
FRONTEND_URL=http://localhost

# Initial admin credentials for idempotent seeding
ADMIN_EMAIL=admin@hotel.com
ADMIN_PASSWORD=password123

# Exposed API URL used by the frontend build
VITE_API_URL=http://backend:3000

# Token for the backup scheduler to call the admin backups endpoint
BACKUP_ADMIN_TOKEN=
